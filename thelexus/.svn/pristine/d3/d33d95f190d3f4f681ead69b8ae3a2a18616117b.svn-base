// main: ../import.less

#st_begin {
  margin-left:180px;
  position:relative;
  height:100%;
///////////////////Youtube 팝업
  .popopen {
    position:fixed;
    width:1903px;
    height:950px;
    z-index:5;
    top:0;
    right:0;
    left:0;
    bottom:0;
    // .close_btn {
    //   z-index:10;
      a {
        z-index:10;
        width:60px;
        height:60px;
        background: url('../../asset/image/service/btn_close_bg.png') repeat 0 0;
        opacity:0.5;
        &:hover {
          opacity:1;
          color:#fff;
        }
        &:link {
          color:#666;
        }
        >.cl_btn {
          left:20px;
          top:20px;
          width:20px;
          height:20px;
          background:url('../../asset/image/service/sprite_common.png') no-repeat -95px -163px;
          text-indent:-9999px;
          font-size:0;
        }
      }
      iframe {
        width:100%;
        height:100%;
      }
    // }
  }
/////////////////////////Youtube 팝업


  .st_begining {
    background-image: url(../../asset/image/story/begining/bg_s0.jpg);
    background-size:cover;
    background-repeat:no-repeat;
    background-attachment:fixed;
    height:100%;
    .begin_content {
      position:absolute;
      left:50%;
      top:50%;
      margin-left:-358.5px;
      margin-top:-90px;
      text-align:right;
      li>img {
        position:absolute;
        top:300px;
        right:20px;
      }
    }
  }
  .birth_begining {
    background-color:#1e1e1e;
    >ul {
      width:717px;
      margin:0 auto;
      text-align:right;
      overflow:hidden;
      li {
        margin:20% 0;
      }
      >li:first-child {
        position:relative;
        >img:first-child {
          position:absolute;
          top:0;
          left:0;
        }

      }


	  >li+li {
	  	>h3 {
	  		position:absolute;
	  		left:24%;
	  	}
	  	>p>img {
	  		margin-top:150px;
	  		margin-right:10px;
	  	}
	  }
      >li:nth-child(3) {
      	position:relative;
      	>p {
      		position:absolute;
      		top:-130px;
      		right:330px;
      	}
      	>img {
      		margin-top:-50px;
      	}
      }
      >li:nth-child(4) {
      	>p:first-child {
      		margin-top:-200px;
      	}
      }

    }
  }

  .launching_begining {
  	background-color:#1e1e1e;
  	height:2400px;
  	overflow:hidden;
  	>ul {
  		position:relative;
  		top:150px;
  		>li:nth-child(1) {
	    background: #000;
  		>div {
  			background-image:url(../../asset/image/story/begining/bg_s3.jpg);
            background-repeat:no-repeat;
            height:360px;
            margin-left:270px;
  			>h3 {
  			position:absolute;
		    width:717px;
		    left:50%;
		    margin-left:-440px;
		    top:-125px;
  			}
  			>p {
  			width:717px;
		    margin:0 auto;
		    text-align:right;
		    margin-left:240px;
		    padding-top:5%;
  			}
  			}
  		}
  		>li+li {
  			width:717px;
    		margin:5% auto;
  		}
  		>li:nth-child(2) {
  			width:717px;
    		margin:4% auto;
    		margin-bottom:130px;
    		>p {
    			float:left;
    		}
    		>img {
    			margin-left:40px;
    		}
  		}
  		>li:nth-child(3) {
  			>a {
          position:relative;
          >img {
            position:relative;
        		margin-top:10%;
          }
    		>img+img {
    			position:absolute;
    			left:51%;
    			bottom:148px;
    			margin-left:-33px;
    			margin-top:-33px;
    		}
  			}

  		}
  		>li:nth-child(4) {
  			position:relative;
  			>span {
  				position:absolute;
			    margin:0 auto;
			    top:33%;
			    left:50%;

  			}
  		}
  	}


}

}
