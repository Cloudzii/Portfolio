  <script type="text/javascript">
		

		$(function(){
	    	 $(".story-wrapper li a").click(function(event){ // li a를 클릭했을 때
	    		 event.preventDefault(); //a의 넘어가려고 하는 특성을 막아주고 animate만 실행되게
	    		 var thish = $(this.hash).offset().top;
	    		 // $(this.hash) this(현재 클릭한 녀석)의 id값
	    		 // .offset()의 좌표
	    		 // 중의 .top; 탑값
	    		 $('html,body').stop().animate({scrollTop:thish}, 1000);
	    		 // $('html,body') 가 이동할 때
	    		 // animate 실행
	    		 // scrollTop 스크롤 탑 값을 :thish 만큼 이동
	    		 // 1000의 duration(시간)동안
	    	 });
	     });
		
		
		$(window).load(function(){
			//target setting 
			 var $tg = $('#hybrid');
	    	 var $tgChild = $tg.find('.parellex_sec');
	    	 
	    	 var secSpot = [];
	    	 var wrap_height = 0
	    	 
	    	 //organize
	    	 $tgChild.each(function(i,e){
	    		 secSpot.push($(e).offset().top); //offset is location value
	    	});
	    	 
	    	$(window).scroll(function(){
	    		
	    		var sct = $(this).scrollTop();
	    		
	    		$tgChild.each(function(i,e){
	    			if(sct > secSpot[i]){
	    				$(e).addClass('on');
	    				//모션 동작
	    			    switch(i){
		    			    case 0:
		    			    	//console.log("실행1")
		    			    	 $('.strip_hybrid_1').css('top',perellex('fx',sct,0.5,$('.strip_hybrid_1')))
		    			    	 $('.man_hybrid_1').css('top',perellex('fx',sct,0.3,$('.man_hybrid_1')))
		    			    	 $('#hybrid_1 h2').css('opacity',perellex('op',sct,0.3,$('#hybrid_1 h2')))
		    			    break;
		    			    case 1:
		    			    	//console.log("실행2")
		    			    	
		    			    break;
		    			    case 2:
		    			    	//console.log("실행3")
		    			    	
		    			    break;
		    			    case 3:
		    			    	//console.log("실행4")
		    			    	
		    			    break;
		    			    case 4:
		    			    	//console.log("실행5")
		    			    	
		    			    break;
		    			    case 5:
		    			    	//console.log("실행6")
		    			    break;
	    			    
	    			    }
	    			    
	    			}
	    		})
	    	});
	    	
	    	
	    	function perellex(eff,x,damage,item){
	    		
	    		if(eff == "fx"){
	    			unit = item.position().top
		    		var y = -1 * x + unit ;
		    		y *= damage;
		    		return y;
		    		
	    		}else if(eff == "op"){
	    			
	    			var xx = item.css('opacity');
	    			
	    			if(x > item.offset().top){
	    				console.log(xx)
	    				return xx -=0.1 
	    			}else if(x < item.offset().top){
	    				console.log(xx)
	    				return xx +=0.1
	    			}
	    		}
	    	}
	    });
  
  </script>
  
